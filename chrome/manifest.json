{
  "name": "Words Collector",
  "description": "Collects words",
  "version": "1.0.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icons/16.png"
  },
  "permissions": [
    "contextMenus",
    "tabs",
    "activeTab",
    "scripting",
    "identity"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/16.png",
    "19": "icons/19.png",
    "32": "icons/32.png",
    "38": "icons/38.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "options_page": "options.html",
  "oauth2": {
    "client_id": "969392774646-lbqtov56l972akmt5srulp1dhga6pked.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets.readonly"
    ]
  }
}